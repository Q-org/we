{
  "index_name": "dev_index",
  "start_urls": ["http://host.docker.internal:83/"],
  "selectors": {
    "lvl0": {
      "selector": "h1",
      "default_value": "Documentation"
    },
    "lvl1": "h2",
    "lvl2": "h3",
    "lvl3": "h4",
    "content": "p, li, blockquote, pre, code, strong, em, ul, ol"
  },
  "strip_chars": "",
  "custom_settings": {
    "searchableAttributes": [
      // 替换 attributesToIndex
      "unordered(lvl0)",
      "unordered(lvl1)",
      "unordered(lvl2)",
      "unordered(lvl3)",
      "unordered(content)"
    ]
  }
}
